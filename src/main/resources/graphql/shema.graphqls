type Person{
    id: ID!,
    name: String,
    age: Int,
    gender: Gender
}

enum Gender {
    MALE
    FEMALE
}

input PersonInput{
    name: String,
    age: Int,
    gender: Gender
}

type Query{
    findAllPeople: [Person]
    findPersonById(id: Int): Person
}

type Mutation{
    savePerson(person: PersonInput): Person
    updateAge(personId: Int, age: Int): Person
}